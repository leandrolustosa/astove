@{
    Layout = null;
}

<div class="modal-body" style="z-index:1051">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>{{title('Requisição inválida')}}</h5>

                    <div ng-include="'/common/astovetoolsclose'"></div>
                </div>
                <div class="ibox-content">
                    
                    <div class="col-md-4 astove-error-content-image">
                        <span class="fa fa-exclamation-triangle fa-5x text-danger"></span>
                    </div>

                    <div class="col-md-8 astove-error-content">
                        <h4>{{message}}</h4>
                        <div ng-bind-html="errorList()"></div>
                    </div>

                </div>
            </div>
        </div>
    </div>
</div>